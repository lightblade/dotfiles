function g:Undotree_CustomMap()
    nmap <buffer> q <plug>UndotreeClose <bar> <Cmd>UndotreeCustomMainClose<CR>
    nmap <buffer> <C-h> echo ""
    nmap <buffer> <C-l> echo ""
    nmap <buffer> <C-j> echo ""
    nmap <buffer> <C-k> echo ""
    nmap <buffer> <M-h> echo ""
    nmap <buffer> <M-l> echo ""
    nmap <buffer> <M-j> echo ""
    nmap <buffer> <M-k> echo ""
endfunc
